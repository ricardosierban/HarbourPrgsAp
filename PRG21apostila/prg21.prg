/*
    Exemplo de operadores numericos
*/
PROCEDURE Main 
LOCAL nNumero

    ? "Tabuada (Soma)"
    INPUT "Informe um numero entre 1 e 9: " TO nNumero

    ? "Soma"
    ? "----"
    ? nNumero, " + 1 = " , nNumero + 1
    ? nNumero, " + 2 = " , nNumero + 2
    ? nNumero, " + 3 = " , nNumero + 3
    ? nNumero, " + 4 = " , nNumero + 4
    ? nNumero, " + 5 = " , nNumero + 5
    ? nNumero, " + 6 = " , nNumero + 6
    ? nNumero, " + 7 = " , nNumero + 7
    ? nNumero, " + 8 = " , nNumero + 8
    ? nNumero, " + 9 = " , nNumero + 9
RETURN 